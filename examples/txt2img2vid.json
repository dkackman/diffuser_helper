{
    "id": "txt2img2vid",
    "steps": [
        {
            "name": "image_generation",
            "pipeline": {
                "transformer": {
                    "configuration": {
                        "pipeline_type": "SD3Transformer2DModel",
                        "bits_and_bytes_configuration": {
                            "load_in_4bit": true,
                            "bnb_4bit_quant_type": "{nf4}",
                            "bnb_4bit_compute_dtype": "torch.bfloat16"
                        }
                    },
                    "from_pretrained_arguments": {
                        "model_name": "stabilityai/stable-diffusion-3.5-large",
                        "subfolder": "transformer",
                        "torch_dtype": "torch.bfloat16"
                    }
                },
                "configuration": {
                    "pipeline_type": "StableDiffusion3Pipeline",
                    "offload": "full"
                },
                "from_pretrained_arguments": {
                    "model_name": "stabilityai/stable-diffusion-3.5-large",
                    "torch_dtype": "torch.bfloat16"
                },
                "arguments": {
                    "prompt": "A steaming mug of coffee. The steam forms the letters XCH",
                    "guidance_scale": 4.5,
                    "num_inference_steps": 25,
                    "height": 480,
                    "width": 720,
                    "max_sequence_length": 512,
                    "num_images_per_prompt": 4
                }
            },
            "result": {
                "content_type": "image/jpeg"
            }
        },
        {
            "name": "image_to_video",
            "pipeline": {
                "configuration": {
                    "offload": "sequential",
                    "pipeline_type": "CogVideoXImageToVideoPipeline",
                    "vae": {
                        "enable_slicing": true,
                        "enable_tiling": true
                    }
                },
                "from_pretrained_arguments": {
                    "model_name": "THUDM/CogVideoX-5b-I2V",
                    "torch_dtype": "torch.bfloat16",
                    "use_safe_tensors": true
                },
                "arguments": {
                    "image": "previous_result:image_generation",
                    "prompt": "the steam rises from the mug",
                    "num_videos_per_prompt": 1,
                    "num_inference_steps": 25,
                    "num_frames": 49,
                    "guidance_scale": 6
                }
            },
            "result": {
                "content_type": "video/mp4"
            }
        }
    ]
}