{
    "id": "sd35",
    "steps": [
        {
            "name": "image_generation",
            "pipeline": {
                "configuration": {
                    "pipeline_type": "DiffusionPipeline",
                    "offload": "sequential"
                },
                "from_pretrained_arguments": {
                    "model_name": "shuttleai/shuttle-3-diffusion",
                    "torch_dtype": "torch.bfloat16"
                }
            },
            "iterations": [
                {
                    "result_properties": {
                        "content_type": "image/jpeg"
                    },
                    "arguments": {
                        "prompt": "portrait | wide angle shot of eyes off to one side of frame, lucid dream-like 3d model of marmot, game asset, blender, looking off in distance ::8 style | glowing ::8 background | forest, vivid neon wonderland, particles, blue, green, orange ::7 parameters | rule of thirds, golden ratio, assymetric composition, hyper- maximalist, octane render, photorealism, cinematic realism, unreal engine, 8k ::7 --ar 16:9 --s 1000",
                        "height": 1024,
                        "width": 1024,
                        "guidance_scale": 3.5,
                        "num_inference_steps": 4,
                        "max_sequence_length": 256
                    },
                    "capture_intermediate_results": {
                        "base_image": "image"
                    }
                }
            ]
        },
        {
            "name": "image_to_video",
            "pipeline": {
                "configuration": {
                    "offload": "sequential",
                    "pipeline_type": "CogVideoXImageToVideoPipeline",
                    "vae": {
                        "enable_slicing": true,
                        "enable_tiling": true
                    }
                },
                "from_pretrained_arguments": {
                    "model_name": "THUDM/CogVideoX-5b-I2V",
                    "torch_dtype": "torch.bfloat16",
                    "use_safe_tensors": true
                }
            },
            "iterations": [
                {
                    "result_properties": {
                        "content_type": "video/mp4",
                        "file_name": "marmot.mp4"
                    },
                    "intermediate_results": {
                        "image": "{base_image}"
                    },
                    "arguments": {
                        "prompt": "The marmot stares intently and blinks",
                        "num_videos_per_prompt": 1,
                        "num_inference_steps": 25,
                        "num_frames": 49,
                        "guidance_scale": 6
                    }
                }
            ]
        }
    ]
}