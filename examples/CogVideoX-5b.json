{
    "template": {
        "applies_to": "text_to_video",
        "variables": {
            "prompts": [
                "A marmot, dressed in a small, red jacket and a tiny hat, holding a sign that says 'soon'"
            ]
        },
        "iteration": {
            "result_properties": {
                "content_type": "video/mp4"
            },
            "arguments": {
                "prompt": "$(prompts)",
                "num_videos_per_prompt": 1,
                "num_inference_steps": 35,
                "num_frames": 49,
                "guidance_scale": 6
            }
        }
    },
    "jobs": [
        {
            "id": "CogVideoX-5b",
            "steps": [
                {
                    "name": "text_to_video",
                    "pipeline": {
                        "configuration": {
                            "offload": "sequential",
                            "pipeline_type": "CogVideoXPipeline",
                            "vae": {
                                "enable_slicing": true,
                                "enable_tiling": true
                            }
                        },
                        "from_pretrained_arguments": {
                            "model_name": "THUDM/CogVideoX-5b",
                            "torch_dtype": "torch.bfloat16"
                        }
                    },
                    "iterations": []
                }
            ]
        }
    ]
}