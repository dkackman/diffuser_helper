{
    "id": "HunyuanVideo",
    "steps": [
        {
            "name": "text_to_video",
            "pipeline": {
                "configuration": {
                    "pipeline_type": "HunyuanVideoPipeline",
                    "offload": "model",
                    "vae": {
                        "enable_slicing": true,
                        "enable_tiling": true
                    }
                },
                "transformer": {
                    "configuration": {
                        "pipeline_type": "HunyuanVideoTransformer3DModel"
                    },
                    "gguf_configuration": {
                        "compute_dtype": "torch.bfloat16"
                    },
                    "from_pretrained_arguments": {
                        "from_single_file": "https://huggingface.co/city96/HunyuanVideo-gguf/blob/main/hunyuan-video-t2v-720p-Q5_K_M.gguf",
                        "torch_dtype": "torch.bfloat16"
                    }
                },
                "from_pretrained_arguments": {
                    "model_name": "hunyuanvideo-community/HunyuanVideo",
                    "torch_dtype": "torch.bfloat16"
                },
                "arguments": {
                    "prompt": "A marmot walks on the snow, realistic",
                    "height": 320,
                    "width": 512,
                    "num_inference_steps": 30,
                    "num_frames": 129
                }
            },
            "result": {
                "content_type": "video/mp4",
                "fps": 15
            }
        }
    ]
}