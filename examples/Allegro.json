{
    "id": "AllegroVideo",
    "steps": [
        {
            "name": "text_to_video",
            "pipeline": {
                "configuration": {
                    "offload": "sequential",
                    "pipeline_type": "AllegroPipeline",
                    "vae": {
                        "enable_tiling": true
                    }
                },
                "vae": {
                    "configuration": {
                        "pipeline_type": "AutoencoderKLAllegro"
                    },
                    "from_pretrained_arguments": {
                        "model_name": "rhymes-ai/Allegro",
                        "subfolder": "vae",
                        "torch_dtype": "torch.float32"
                    }
                },
                "from_pretrained_arguments": {
                    "model_name": "rhymes-ai/Allegro",
                    "torch_dtype": "torch.bfloat16"
                },
                "arguments": {
                    "prompt": "A woodland scene with bright sunlight and sparkling sunshine through the leaves, with many faeries. From an aerial view, the faeries vary in size and color, some moving and some stationary. The faeries wear boots.",
                    "guidance_scale": 7.5,
                    "max_sequence_length": 512,
                    "num_inference_steps": 30
                }
            },
            "result": {
                "content_type": "video/mp4"
            }
        }
    ]
}