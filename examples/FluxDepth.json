{
    "variables": {
        "input_image": "https://pbs.twimg.com/card_img/1861062981414461440/ksfE0eTn?format=jpg&name=small",
        "robot_image": "https://huggingface.co/datasets/huggingface/documentation-images/resolve/main/robot.png",
        "mondo_image": "https://pbs.twimg.com/media/GeynVQNbwAAYtRt?format=jpg&name=medium"
    },
    "jobs": [
        {
            "id": "FluxDepth",
            "steps": [
                {
                    "name": "depth",
                    "task": {
                        "command": "depth",
                        "arguments": {
                            "image": {
                                "location": "variable:mondo_image"
                            }
                        }
                    },
                    "result": {
                        "content_type": "image/jpeg"
                    }
                },
                {
                    "name": "FluxDepth",
                    "pipeline": {
                        "configuration": {
                            "pipeline_type": "FluxControlPipeline",
                            "offload": "sequential"
                        },
                        "from_pretrained_arguments": {
                            "model_name": "black-forest-labs/FLUX.1-Depth-dev",
                            "torch_dtype": "torch.bfloat16"
                        },
                        "arguments": {
                            "control_image": "previous_result:depth",
                            "prompt": "A Chimpanzee dressed as super man flying over a city",
                            "height": 1024,
                            "width": 1024,
                            "guidance_scale": 10,
                            "num_inference_steps": 25,
                            "max_sequence_length": 512
                        }
                    },
                    "result": {
                        "content_type": "image/jpeg"
                    }
                }
            ]
        }
    ]
}