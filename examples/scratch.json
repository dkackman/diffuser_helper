{
    "variables": {
        "prompt": "A marmot, wearing a tophat in a woodland setting. Somewhat magical."
    },
    "id": "PromptAugmentation",
    "steps": [
        {
            "name": "augment_prompt",
            "workflow": {
                "path": "builtin:augment_prompt.json",
                "arguments": {
                    "prompt": "variable:prompt"
                }
            },
            "result": {
                "content_type": "application/json"
            }
        },
        {
            "name": "main",
            "pipeline": {
                "configuration": {
                    "pipeline_type": "FluxPipeline",
                    "offload": "sequential"
                },
                "from_pretrained_arguments": {
                    "model_name": "black-forest-labs/FLUX.1-dev",
                    "torch_dtype": "torch.bfloat16"
                },
                "arguments": {
                    "prompt": "previous_result:augment_prompt.generated_text",
                    "num_inference_steps": 25,
                    "num_images_per_prompt": 1,
                    "guidance_scale": 3.5,
                    "max_sequence_length": 512
                }
            },
            "result": {
                "content_type": "image/jpeg"
            }
        }
    ]
}