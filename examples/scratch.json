{
    "jobs": [
        {
            "id": "FluxInContext",
            "steps": [
                {
                    "name": "gather_images",
                    "task": {
                        "command": "gather_images",
                        "arguments": {
                            "glob": "./outputs/OneMarmotLogo.png"
                        }
                    }
                },
                {
                    "name": "remove_background",
                    "pipeline": {
                        "configuration": {
                            "pipeline_type": "FluxPipeline",
                            "offload": "sequential"
                        },
                        "from_pretrained_arguments": {
                            "model_name": "black-forest-labs/FLUX.1-dev",
                            "torch_dtype": "torch.bfloat16"
                        },
                        "arguments": {
                            "prompt": "The two-panel image showcases the joyful identity of a blockchain brand, with the left panel showing a smiling marmot graphic and the brand name “One Marmot in a fun, casual font on a light green background; [LEFT] while the right panel translates the design onto a reusable shopping tote with the marmot logo in brown, held by a person in a market setting, emphasizing the brand’s approachable and eco-friendly vibe.",
                            "width": 1472,
                            "height": 1024,
                            "num_inference_steps": 25,
                            "num_images_per_prompt": 1,
                            "guidance_scale": 3.5,
                            "max_sequence_length": 512
                        }
                    },
                    "result": {
                        "content_type": "image/jpeg"
                    }
                }
            ]
        }
    ]
}