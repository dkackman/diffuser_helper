                    },
                    "arguments": {
                        "image": {
                            "location": "https://pbs.twimg.com/media/GcnSunDWIAAF0jL?format=jpg&name=medium"
                        },
                        "prompt": "laughing",
                        "num_videos_per_prompt": 1,
                        "num_inference_steps": 25,
                        "num_frames": 49,
                        "guidance_scale": 6
                    }
                }
            ]
        }
    ]
