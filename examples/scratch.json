{
    "id": "workflow",
    "steps": [
        {
            "name": "gather_images",
            "task": {
                "command": "gather",
                "arguments": {
                    "glob": "./test_images/*.jpg"
                }
            }
        },
        {
            "name": "resize",
            "task": {
                "command": "center_crop",
                "arguments": {
                    "image": "previous_result:gather_images",
                    "height": 128,
                    "width": 128
                }
            },
            "result": {
                "content_type": "image/jpeg"
            }
        }
    ]
}