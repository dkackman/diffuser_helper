{
    "variables": {
        "image_locations": [
            "https://pbs.twimg.com"
        ]
    },
    "template": {
        "applies_to": "image_to_video",
        "variables": {
            "image_locations": [
                "https://media.discordapp.net/attachments/1170349328225800304/1307324253565091890/CHAD.jpg?ex=67407b96&is=673f2a16&hm=0bd352d5b365a41074640a71569a3b51f73e0cf53d9a63feafd856cd043fdbc0&=&format=webp&width=597&height=597"
            ]
        },
        "iteration": {
            "result_properties": {
                "content_type": "video/mp4"
            },
            "arguments": {
                "image": {
                    "location": "$(image_locations)"
                },
                "prompt": "the man laughs",
                "num_videos_per_prompt": 1,
                "num_inference_steps": 25,
                "num_frames": 49,
                "guidance_scale": 6
            }
        }
    },
    "jobs": [
        {
            "id": "CogVideoX-5B-I2V",
            "steps": [
                {
                    "name": "image_to_video",
                    "pipeline": {
                        "configuration": {
                            "offload": "sequential",
                            "pipeline_type": "CogVideoXImageToVideoPipeline",
                            "vae": {
                                "enable_slicing": true,
                                "enable_tiling": true
                            }
                        },
                        "from_pretrained_arguments": {
                            "model_name": "THUDM/CogVideoX-5B-I2V",
                            "torch_dtype": "torch.bfloat16",
                            "use_safe_tensors": true
                        }
                    },
                    "iterations": []
                }
            ]
        }
    ]
}