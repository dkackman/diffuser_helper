{
    "template": {
        "applies_to": "image_to_video",
        "variables": {
            "image_locations": [
                "https://pbs.twimg.com/media/GdeUpIiXQAEvYQD?format=jpg&name=small"
            ]
        },
        "iteration": {
            "result": {
                "content_type": "video/mp4"
            },
            "arguments": {
                "image": {
                    "location": "$(image_locations)"
                },
                "prompt": "the lizard fixes the rocket",
                "num_videos_per_prompt": 1,
                "num_inference_steps": 25,
                "num_frames": 49,
                "guidance_scale": 2.5
            }
        }
    },
    "jobs": [
        {
            "id": "CogVideoX-5B-I2V",
            "steps": [
                {
                    "name": "image_to_video",
                    "pipeline": {
                        "configuration": {
                            "offload": "sequential",
                            "pipeline_type": "CogVideoXImageToVideoPipeline",
                            "vae": {
                                "enable_slicing": true,
                                "enable_tiling": true
                            }
                        },
                        "from_pretrained_arguments": {
                            "model_name": "THUDM/CogVideoX-5B-I2V",
                            "torch_dtype": "torch.bfloat16",
                            "use_safe_tensors": true
                        }
                    }
                }
            ]
        }
    ]
}