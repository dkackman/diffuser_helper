{
    "variables": {
        "image": {
            "location": "https://huggingface.co/datasets/huggingface/documentation-images/resolve/main/transformers/tasks/car.jpg?download:true"
        },
        "prompt": "default"
    },
    "id": "CogVideoX-5B-I2V",
    "steps": [
        {
            "name": "image_to_video",
            "pipeline": {
                "configuration": {
                    "offload": "sequential",
                    "pipeline_type": "CogVideoXImageToVideoPipeline",
                    "vae": {
                        "enable_slicing": true,
                        "enable_tiling": true
                    }
                },
                "from_pretrained_arguments": {
                    "model_name": "THUDM/CogVideoX-5B-I2V",
                    "torch_dtype": "torch.bfloat16"
                },
                "arguments": {
                    "image": "variable:image",
                    "prompt": "variable:prompt",
                    "num_videos_per_prompt": 1,
                    "num_inference_steps": 50,
                    "num_frames": 49,
                    "guidance_scale": 6
                }
            },
            "result": {
                "content_type": "video/mp4"
            }
        }
    ]
}