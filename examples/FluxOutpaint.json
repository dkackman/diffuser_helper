{
    "variables": {
        "input_image": "https://pbs.twimg.com/media/GddfB1aXwAA6K5c?format=jpg&name=medium",
        "cup_mask_image": "https://huggingface.co/datasets/diffusers/diffusers-images-docs/resolve/main/cup_mask.png",
        "robot_image": "https://huggingface.co/datasets/huggingface/documentation-images/resolve/main/robot.png",
        "mondo_image": "https://pbs.twimg.com/media/GdcxG6oaUAAHgSL?format=jpg&name=medium"
    },
    "jobs": [
        {
            "id": "FluxOutpaint",
            "steps": [
                {
                    "name": "outpaint",
                    "preprocessors": [
                        {
                            "name": "add_border_and_mask",
                            "result_name": "image_with_border",
                            "image": {
                                "location": "$(input_image)"
                            },
                            "arguments": {
                                "zoom_all": 1.5
                            }
                        }
                    ],
                    "pipeline": {
                        "configuration": {
                            "pipeline_type": "FluxFillPipeline",
                            "offload": "sequential"
                        },
                        "from_pretrained_arguments": {
                            "model_name": "black-forest-labs/FLUX.1-Fill-dev",
                            "torch_dtype": "torch.bfloat16"
                        },
                        "result": {
                            "content_type": "image/jpeg"
                        },
                        "intermediate_results": {
                            "image": "image_with_border.bordered_image",
                            "mask_image": "image_with_border.mask"
                        },
                        "arguments": {
                            "prompt": "",
                            "guidance_scale": 30,
                            "num_inference_steps": 25,
                            "max_sequence_length": 512
                        }
                    }
                }
            ]
        }
    ]
}