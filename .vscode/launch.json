{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "test-all",
      "type": "debugpy",
      "request": "launch",
      "module": "dh.run",
      "cwd": "${workspaceFolder}/",
      "justMyCode": true,
      "args": [
        "./examples/tests.json"
      ]
    },
    {
      "name": "scratch",
      "type": "debugpy",
      "request": "launch",
      "module": "dh.run",
      "cwd": "${workspaceFolder}/",
      "justMyCode": true,
      "args": [
        "./examples/scratch.json",
      ]
    },
    {
      "name": "validate file",
      "type": "debugpy",
      "request": "launch",
      "module": "dh.validate",
      "cwd": "${workspaceFolder}/",
      "justMyCode": true,
      "args": [
        "./examples/${input:jobName}.json"
      ]
    },
    {
      "name": "validate tests file",
      "type": "debugpy",
      "request": "launch",
      "module": "dh.validate",
      "cwd": "${workspaceFolder}/",
      "justMyCode": true,
      "args": [
        "./examples/tests.json"
      ]
    },
    {
      "name": "test",
      "type": "debugpy",
      "request": "launch",
      "module": "dh.test",
      "cwd": "${workspaceFolder}/",
      "justMyCode": true
    },
    {
      "name": "test-with-variable",
      "type": "debugpy",
      "request": "launch",
      "module": "dh.run",
      "cwd": "${workspaceFolder}/",
      "justMyCode": true,
      "args": [
        "./dh/test_job.json",
        "prompt=an orange",
        "num_images_per_prompt=4"
      ]
    },
    {
      "name": "ask",
      "type": "debugpy",
      "request": "launch",
      "module": "dh.run",
      "cwd": "${workspaceFolder}/",
      "justMyCode": true,
      "args": [
        "./examples/${input:jobName}.json"
      ]
    }
  ],
  "inputs": [
    {
      "id": "jobName",
      "type": "promptString",
      "description": "Enter the job name:",
      "default": "scratch"
    }
  ]
}